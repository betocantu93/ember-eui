{{#if @buttonEmpty}}
  <EuiButtonEmpty
    @color={{@color}}
    @iconType={{@iconType}}
    @flush={{@flush}}
    @href={{@href}}
    {{on "click" (optional @onClick)}}
  >
    {{@title}}
  </EuiButtonEmpty>
{{else}}
  <EuiButton
    @color={{@color}}
    @iconType={{@iconType}}
    @flush={{@flush}}
    @href={{@href}}
    {{on "click" (optional @onClick)}}
  >
    {{@title}}
  </EuiButton>
{{/if}}